import wollok.game.*
import personaje.*
import constantes.*

//import pc.*
program inicio {
	game.width(30)
	game.height(20)
	game.cellSize(32)
	game.title("Wollok Tycoon")
	game.boardGround("escena.png")
	
	game.addVisual(jugador)
	
	game.start()
	
	keyboard.up().onPressDo({ jugador.moverseArriba() })
